#include<iostream>
using namespace std;

int main()
{
   int age;
   cout<<"Enter your age: ";
   cin>>age;
   
   //person is teenager or not
   //>=13 and <=19
   if(age>=13 && age<=19)
   {
      cout<<"person is teenager"<<endl;
   }
   else
   {
      cout<<"person is not a teenager"<<endl;
   }
   
   //condition to check voting eligility
   if(age>=18)
   {
      cout<<"person is eligible for voting"<<endl;
   }
   else
   {
      cout<<"person is not eligible for voting"<<endl;
   }
   return 0;
}  
